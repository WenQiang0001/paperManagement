<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.papermanagementbe.mapper.StudentMapper">

    <resultMap id="BaseResultMap" type="com.example.papermanagementbe.schema.StudentSchema">
            <id property="id" column="Id" jdbcType="INTEGER"/>
            <result property="studentName" column="StudentName" jdbcType="VARCHAR"/>
            <result property="studentNo" column="StudentNo" jdbcType="VARCHAR"/>
            <result property="studentAccount" column="StudentAccount" jdbcType="VARCHAR"/>
            <result property="studentPassword" column="StudentPassword" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        Id,StudentName,StudentNo,
        StudentAccount,StudentPassword
    </sql>
</mapper>
